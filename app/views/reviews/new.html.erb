<h1>Review</h1>

<%= simple_form_for [ @restaurant, @review ] do |f| %>
  <%= f.input :content %>
  <%= f.select :rating, options_for_select(%w[0 1 2 3 4 5]) %>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
<% end %>

<p> <%= link_to("Back to All Restaurants", restaurants_path) %> </p>